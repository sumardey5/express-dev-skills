<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Skills</title>
</head>
<body>
    <h1>Skills as of <%= time %></h1>
    <ul>
        <% skills.forEach(function(s, idx) { %>
            <li>
                <a href="/skills/<%=idx%>"><%=s.skill%></a>
                <%= s.skill %>
                - 
                <%= s.level ? 'advanced' : 'beginner' %>
            </li>
        <% }); %> 
    </ul>
</body>
</html>